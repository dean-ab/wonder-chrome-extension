{
  "manifest_version": 3,
  "name": "Wonder AI - Your GPT writing assistant",
  "version": "1.1.3",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "js": ["./src/content.tsx"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": ["storage"]
}
